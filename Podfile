target 'Hotel Life' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  inherit! :search_paths
  # Pods for Hotel Life
  #pod 'XLPagerTabStrip', '7.0.0'
  pod 'Firebase/Core'
  pod 'FBSDKLoginKit'
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'TwitterKit5'
  pod 'IQKeyboardManagerSwift'
  pod 'ReachabilitySwift'
  pod 'SDWebImage', '~> 4.0'
  pod 'MRCountryPicker', '~> 0.0.7'
  pod 'Fabric'
  pod 'Crashlytics'
  pod 'CircleProgressBar'
  pod 'Socket.IO-Client-Swift', '~> 13.3.1'
  pod 'FSCalendar'
  pod 'AuthorizeNetAccept'
  
  workspace 'Hotel Life.xcworkspace'
  project 'Hotel Life.xcodeproj'
  #applibs
  
  target 'Hotel LifeTests' do
    inherit! :search_paths
    # Pods for testing
  end
  
  target 'Hotel LifeUITests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end

